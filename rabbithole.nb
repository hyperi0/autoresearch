Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "find", " ", "top", " ", "links", " ", "based", " ", "on", " ", "PageRank",
     " ", "algorithm"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"getLinkOrder", "[", "linkRules_", "]"}], ":=", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"graph", "=", 
