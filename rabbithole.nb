          "Greater"}], "]"}]}], "]"}]}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{"getTopLinks", "[", 
     RowBox[{"linkRules_", ",", 
      RowBox[{"n_", ":", "50"}]}], "]"}], ":=", 
    RowBox[{
     RowBox[{"getLinkOrder", "[", "linkRules", "]"}], "[", 
     RowBox[{"[", 
      RowBox[{";;", 
       RowBox[{"UpTo", "[", "n", "]"}]}], "]"}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{"getTopLinkRules", "[", 
     RowBox[{"linkRules_", ",", 
