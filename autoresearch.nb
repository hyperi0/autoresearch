Cell[BoxData[
 RowBox[{
  RowBox[{"sentenceSimilarity", "[", 
   RowBox[{"s1_", ",", "s2_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"c1", "=", 
        RowBox[{"getWords", "[", "s1", "]"}]}], ",", 
       RowBox[{"c2", "=", 
        RowBox[{"getWords", "[", "s2", "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Length", "[", 
       RowBox[{"Intersection", "[", 
        RowBox[{"c1", ",", "c2"}], "]"}], "]"}], "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Log", "[", 
         RowBox[{"Length", "[", "c1", "]"}], "]"}], "+", 
        RowBox[{"Log", "[", 
         RowBox[{"Length", "[", "c2", "]"}], "]"}]}], ")"}]}]}], "]"}], "//", 
   "N"}]}]], "Input",ExpressionUUID->"d1255b77-053b-4c40-85b6-f0f5a292ec91"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"get", " ", "article", " ", "key", " ", "sentences"}], " ", "*)"}],
   "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"getWords", "[", "text_", "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"StringDelete", "[", 
       RowBox[{"text", ",", "PunctuationCharacter"}], "]"}], "//", 
      "ToLowerCase"}], "//", "StringSplit"}]}], "\n", 
   RowBox[{
    RowBox[{"filterWords", "[", "words_", "]"}], ":=", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"pos", "=", 
        RowBox[{"Association", "@@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"#", "->", 
              RowBox[{"WordData", "[", 
               RowBox[{"#", ",", "\"\<PartsOfSpeech\>\""}], "]"}]}], ")"}], 
            "&"}], "/@", "words"}], ")"}]}]}], "}"}], ",", 
      "\[IndentingNewLine]", "\t", 
      RowBox[{"Select", "[", 
       RowBox[{
        RowBox[{"Keys", "[", "pos", "]"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"ContainsAny", "[", 
           RowBox[{
            RowBox[{"pos", "[", "#", "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"\"\<Noun\>\"", ",", "\"\<Adjective\>\""}], "}"}]}], 
           "]"}], "&&", 
          RowBox[{"ContainsNone", "[", 
           RowBox[{
            RowBox[{"pos", "[", "#", "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"\"\<Preposition\>\"", ",", "\"\<Determiner\>\""}], 
             "}"}]}], "]"}]}], "&"}]}], "]"}]}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{"sentenceSimilarity", "[", 
     RowBox[{"s1_", ",", "s2_", ",", "keywords_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", "\n", "\t", 
        RowBox[{
         RowBox[{"c1", "=", 
          RowBox[{"Intersection", "[", 
           RowBox[{
            RowBox[{"getWords", "[", "s1", "]"}], ",", "keywords"}], "]"}]}], 
         ",", 
         RowBox[{"c2", "=", 
          RowBox[{"Intersection", "[", 
           RowBox[{
            RowBox[{"getWords", "[", "s2", "]"}], ",", "keywords"}], 
           "]"}]}]}], "}"}], ",", "\n", "\t", 
       RowBox[{
        RowBox[{"Length", "[", 
         RowBox[{"Intersection", "[", 
          RowBox[{"c1", ",", "c2"}], "]"}], "]"}], "/", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Log", "[", 
           RowBox[{"Length", "[", "c1", "]"}], "]"}], "+", 
          RowBox[{"Log", "[", 
           RowBox[{"Length", "[", "c2", "]"}], "]"}], "+", ".0000001"}], 
         ")"}]}]}], "]"}], "//", "N"}]}]}]}]], "Code",
 CellChangeTimes->{{3.840991087722425*^9, 3.840991091788781*^9}, {
