Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"getLinks", "[", 
    RowBox[{"article_", ",", "depth_", ",", "breadth_"}], "]"}], ":=", 
   RowBox[{"WikipediaData", "[", 
    RowBox[{"article", ",", "\n", "\t", "\"\<LinksRules\>\"", ",", 
     RowBox[{"\"\<MaxLevel\>\"", "->", "depth"}], ",", 
     RowBox[{"\"\<MaxLevelItems\>\"", "->", "5"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"getLinksGraph", "[", "links_", "]"}], ":=", 
  RowBox[{"Graph", "[", 
   RowBox[{"links", ",", 
    RowBox[{"VertexLabels", "->", 
     RowBox[{"Placed", "[", 
      RowBox[{"\"\<Name\>\"", ",", "Tooltip"}], "]"}]}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"showImportance", "[", 
   RowBox[{"graph_", ",", "func_"}], "]"}], ":=", 
  RowBox[{"HighlightGraph", "[", 
   RowBox[{"graph", ",", 
    RowBox[{"VertexList", "@", "graph"}], ",", "\n", "\t", 
    RowBox[{"VertexSize", "->", 
     RowBox[{"Thread", "[", 
      RowBox[{
       RowBox[{"VertexList", "@", "graph"}], "->", 
       RowBox[{"Rescale", "[", 
        RowBox[{"func", "@", "graph"}], "]"}]}], "]"}]}], ",", 
    RowBox[{"ImageSize", "->", "Medium"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"getOrder", "[", "graph_", "]"}], ":=", 
  RowBox[{"Part", "[", 
   RowBox[{
    RowBox[{"VertexList", "[", "graph", "]"}], ",", 
    RowBox[{"Ordering", "[", 
     RowBox[{
      RowBox[{"PageRankCentrality", "[", "graph", "]"}], ",", "All", ",", 
      "Greater"}], "]"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"getKeywords", "[", 
    RowBox[{"graph_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Take", "[", 
    RowBox[{
     RowBox[{"o", "=", 
      RowBox[{"getOrder", "[", "graph", "]"}]}], ",", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "o", "]"}], ">", "n"}], ",", "n", ",", 
       RowBox[{"Length", "[", "o", "]"}]}], "]"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "getArticleKeywords", "]"}], "=", 
