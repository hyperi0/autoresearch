Cell[BoxData[
 RowBox[{
  RowBox[{"sentenceSimilarity", "[", 
   RowBox[{"s1_", ",", "s2_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"c1", "=", 
        RowBox[{"getWords", "[", "s1", "]"}]}], ",", 
       RowBox[{"c2", "=", 
        RowBox[{"getWords", "[", "s2", "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Length", "[", 
       RowBox[{"Intersection", "[", 
        RowBox[{"c1", ",", "c2"}], "]"}], "]"}], "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Log", "[", 
         RowBox[{"Length", "[", "c1", "]"}], "]"}], "+", 
        RowBox[{"Log", "[", 
         RowBox[{"Length", "[", "c2", "]"}], "]"}]}], ")"}]}]}], "]"}], "//", 
   "N"}]}]], "Input",
