Cell[BoxData[
 RowBox[{
  RowBox[{"filterWords", "[", "text_String", "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"pos", "=", 
      RowBox[{"Association", "@@", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"#", "->", 
            RowBox[{"WordData", "[", 
             RowBox[{"#", ",", "\"\<PartsOfSpeech\>\""}], "]"}]}], ")"}], 
          "&"}], "/@", 
         RowBox[{"StringSplit", "[", 
          RowBox[{"cleanText", "[", "text", "]"}], "]"}]}], ")"}]}]}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{"Select", "[", 
     RowBox[{
      RowBox[{"Keys", "[", "pos", "]"}], ",", 
      RowBox[{
       RowBox[{"ContainsOnly", "[", 
        RowBox[{
         RowBox[{"pos", "[", "#", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"\"\<Noun\>\"", ",", "\"\<Adjective\>\""}], "}"}]}], "]"}], 
       "&"}]}], "]"}]}], "]"}]}]], "Input",ExpressionUUID->"afbfc6bb-bd03-\
47f9-a8a4-f497b56d719f"],

Cell[BoxData[
 RowBox[{
