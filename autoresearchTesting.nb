 RowBox[{
  RowBox[{"getArticleKeywords", "[", 
   RowBox[{"article_", ",", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"getKeywords", "[", 
   RowBox[{
